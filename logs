[30;1m2023-04-16 11:31:41[0m [34;1mINFO    [0m [35mchatgpt_logger[0m -> Ueff707dbb373a21ccefbf2bbe73f4013: /(B) 5節
[30;1m2023-04-16 11:32:45[0m [34;1mINFO    [0m [35mchatgpt_logger[0m -> Ueff707dbb373a21ccefbf2bbe73f4013: B
[30;1m2023-04-16 11:33:43[0m [34;1mINFO    [0m [35mchatgpt_logger[0m -> Ueff707dbb373a21ccefbf2bbe73f4013: 這段程式要如何修該才能在最後傳出答對或答錯的訊息
      questions = [q1, q2, q3, q4, q5]
      
      if questions:
          ran_q = random.choice(questions)
          actions = []
          for option in ['A', 'B', 'C', 'D']:
              action = PostbackTemplateAction(
                  label=ran_q['options'][option],
                  text=f"/({option}) {ran_q['options'][option]}",
                  data=f"{option}&{ran_q['options'][option]}"
              )
              actions.append(action)
          template = ButtonsTemplate(title='題目', text=ran_q['q'], actions=actions)
          question_message = TemplateSendMessage(
              alt_text='題目：' + str(ran_q['q']) + '\n選項：' + str(ran_q['options']),
              template=template
          )
          line_bot_api.reply_message(event.reply_token, question_message)
      else:
          line_bot_api.reply_message(event.reply_token, TextSendMessage(text="抱歉，目前沒有題目。"))
      
      if event.message.text.startswith('/('):
          selected_option = event.message.text[2:3]
          if selected_option in ['A', 'B', 'C', 'D']:
              if selected_option == ran_q['a']:
                  msg_text = "答對了！" + str(ran_q['explain'])
                  print(msg_text)
              else:
                  msg_text = "答錯了！" + str(ran_q['explain'])
                  print(msg_text)
              answer_message = TextSendMessage(text=msg_text)
              line_bot_api.push_message(event.source.user_id, answer_message)
              questions.remove(ran_q) # 從題目列表中移除已回答的題目
          else:
              error_message = TextSendMessage(text="請輸入選項 A、B、C 或 D。")
              line_bot_api.push_message(event.source.user_id, error_message)
